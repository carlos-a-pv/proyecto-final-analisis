@article{TEREFE2025101341,
title = {Prevalence and risk factors of mange mite (Sarcoptes and Demodex) infestations in small ruminants in Wolaita zone, southern Ethiopia},
journal = {Veterinary Parasitology: Regional Studies and Reports},
volume = {64},
pages = {101341},
year = {2025},

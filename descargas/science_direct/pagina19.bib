@article{BERGUS2024102783,
title = {Algorithm for management of stuck ports in pediatric patients: A case report},
journal = {Journal of Pediatric Surgery Case Reports},
volume = {102},
pages = {102783},
year = {2024},
issn = {2213-5766},
doi = {https://doi.org/10.1016/j.epsc.2024.102783},
url = {https://www.sciencedirect.com/science/article/pii/S2213576624000113},
author = {Katherine C. Bergus and Andrew Klingele and Shruthi Srinivas and Peter C. Minneci},
keywords = {Case report, Central venous catheter, Retained fragment, Pediatric},
abstract = {Introduction
Difficulty removing central venous ports in children is generally uncommon. We present a rare case of intraoperative difficulty with port removal that resulted in catheter breakage.
Case presentation
An 18-year-old girl presented for port removal nearly six years after placement. The catheter was stuck intraoperatively and despite increased tension, proximal dissection, and extraction attempts with a wire, a portion of the catheter broke off intravascularly. Since intraoperative fluoroscopy confirmed that neither end of the catheter had free intravascular motion, the catheter was left in place. The patient has had no apparent complications from this retained portion of the catheter.
Conclusion
It is uncommon to have great difficulty removing a port in a child but in cases where the catheter is stuck, multiple techniques can be attempted intraoperatively to improve chances of removal. If the catheter breaks intraoperatively but has no free intravascular motion on fluoroscopy, it may be reasonable to leave the retained fragment in place and obtain interval imaging to ensure the fragment has not moved.}
}
@article{BERGUS2024102835,
title = {Site wedge resection technique for gastrostomy tube site revision: A case series},
journal = {Journal of Pediatric Surgery Case Reports},
volume = {107},
pages = {102835},
year = {2024},
issn = {2213-5766},
doi = {https://doi.org/10.1016/j.epsc.2024.102835},
url = {https://www.sciencedirect.com/science/article/pii/S2213576624000630},
author = {Katherine C. Bergus and Shruthi Srinivas and Peter C. Minneci},
keywords = {Gastrostomy tube, Hypertrophic granulation, Gastric prolapse, Revision, Case series},
abstract = {Introduction
Gastrostomy tube hypertrophic granulation tissue development, prolapse, and leakage are common problems that are often treated with tube re-siting. Wedge gastrostomy tube site resection is a revision technique used to enable continued use of the original tube site while removing affected tissue. The aim of this study was to describe this technique and outcomes in a series of patients requiring this intervention. The wedge gastrostomy tube site resection involves wedge excision of hypergranulation tissue or prolapsed gastric tissue and primary closure of the remaining defect. We reviewed outcomes for two children who underwent wedge gastrostomy tube site resection for medically refractory hypergranulation tissue.
Case presentation
Two patients presented with hypertrophic granulation tissue at their gastrostomy tube sites. In Case 1, the patient underwent two revisions spaced two years apart and has been symptom-free for 9 months. In Case 2, the patient required a single revision and has been symptom-free for 13 months. Neither patient had complications related to their gastrostomy tube site revision in the immediate postoperative period.
Conclusion
Wedge gastrostomy tube site resection is a useful strategy to remove reactive tissue that contributes to leakage around a gastrostomy tube and avoids the morbidities of tube re-siting.
Level of evidence
IV.}
}

@article{SRIVASTAVA2025103128,
title = {Emerging paradigms in the lateral and transverse organization in biological membrane and their functional implications: Connecting the dots with biomolecular simulations},
journal = {Current Opinion in Structural Biology},
volume = {94},
pages = {103128},
year = {2025},
issn = {0959-440X},
doi = {https://doi.org/10.1016/j.sbi.2025.103128},
url = {https://www.sciencedirect.com/science/article/pii/S0959440X25001460},
author = {Anand Srivastava},
abstract = {Since the publication of the first papers in the early 1990s, molecular simulation as a reliable biophysical tool in the area of membrane biophysics has come a long way. Advances in simulation algorithms, coupled with exascale computing have pushed the size and time scales of biomolecular membrane simulations to scales where connections to experiments are made with higher fidelity. When integrated with experimental data in a theoretically well-grounded manner, current biomolecular simulations are providing indispensable insights that cannot be obtained through experiments alone. Here, I summarize some recent developments where simulations have allowed a deeper understanding in membrane spatiotemporal organization. I also discuss the need for transformative method developments to meet recent breakthroughs in experimental measurements at molecular scales.}
}
@article{KAZEMI2025205751,
title = {Enhancing permeability prediction from digital rock images using generative AI and transfer learning},
journal = {Gas Science and Engineering},
volume = {144},
pages = {205751},
year = {2025},
issn = {2949-9089},
doi = {https://doi.org/10.1016/j.jgsce.2025.205751},
url = {https://www.sciencedirect.com/science/article/pii/S2949908925002158},
author = {Alireza Kazemi and Mohammad Esmaeili},
keywords = {Generative AI, Diffusion models, Transfer learning, Image based regression, Computer vision, Digital rock analysis},
abstract = {Permeability is one of the most important properties of porous media, governing fluid flow and transport in subsurface reservoirs. However, accurately calculating permeability through conventional numerical simulations can be computationally expensive, especially when dealing with complex pore structures. Therefore, rapid and precise prediction of permeability from digital rock images using deep learning and image-based regression is of particular interest. Yet, the generalizability and strength of deep learning models in computer vision tasks are often limited by low variability of existing datasets and labeling efforts. To mitigate such challenges, in this work, we introduce a new framework for improving permeability prediction with two complementary strategies. First, we expand the dataset by creating synthetic 2D digital rock images based on a diffusion model, which adds further data to the training set. Second, we embrace transfer learning to leverage pre-trained models, thus increasing predictive performance in terms of permeability estimation tasks even without the presence of a large number of labeled samples. The framework presented demonstrates greater accuracy and generalization compared to traditional approaches, thus facilitating more reliable permeability prediction in the area of digital rock analysis. The innovation demonstrates potential for the use of generative modeling and transfer learning to drive data-driven solutions in subsurface characterization.}
}
@article{CHENG2025,

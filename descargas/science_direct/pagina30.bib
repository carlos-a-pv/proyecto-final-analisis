url = {https://www.sciencedirect.com/science/article/pii/S105752192500626X},
author = {Yu Wang and Fan Dong},
keywords = {Data capitalization, Intellectual property protection, Digital transformation, Data management},
abstract = {While intellectual property (IP) protection provides legal mechanisms to safeguard assets, the majority of previous research has focused on its benefits for traditional assets, neglecting its influence on facilitating data capitalization. Using a longitudinal dataset of Chinese listed firms from 2011 to 2022, this study demonstrates that IP protection has a positive effect on corporate data capitalization by enhancing data capitalization resources and willingness. Moreover, heterogeneity analysis reveals that private firms, high-tech industries, and regions with advanced digital infrastructure are more effective in achieving data capitalization under IP protection. Findings contribute to the literature on IP and data capitalization and provide practical insights for practitioners seeking to optimize data-driven strategies.}
}
@article{ZHOU2025104526,
title = {Generative pre-trained transformers (GPT) for lithium-ion battery charging state prediction in battery swapping station},
journal = {Sustainable Energy Technologies and Assessments},
volume = {82},
pages = {104526},
year = {2025},
issn = {2213-1388},
doi = {https://doi.org/10.1016/j.seta.2025.104526},
url = {https://www.sciencedirect.com/science/article/pii/S2213138825003571},
author = {Dali Zhou and Yufeng Sun and Qin Hu and Ying Shi and Jicheng Yu and Jian Zhang},
keywords = {Battery swapping, Lithium-ion battery, Charging energy prediction, Time series prediction, Large language model},
abstract = {Battery swapping stations face considerable challenges due to heterogeneous battery types, inconsistent initial states, and complex spatiotemporal patterns in BMS data. To address these issues, we propose BatteryGPT, a frozen pre-trained Generative Pre-trained Transformer (GPT) framework tailored for lithium-ion battery energy forecasting. Instead of retraining the entire model, our approach fine-tunes only the input embedding and output projection layers, enabling efficient adaptation to varied battery conditions. To enhance temporal feature representation, we introduce a contrastive temporal embedding module that compresses multivariate sequences while retaining essential dynamic features. Furthermore, we design a temporal suffix alignment strategy to align time-series data with textual prompts, improving the model’s capacity for temporal reasoning. Experiments show that BatteryGPT achieves an average 55.52% improvement in prediction accuracy over LSTM and conventional deep learning baselines. Instruction-based evaluations further demonstrate its end-to-end applicability in dynamic charging management scenarios. These results highlight the potential of integrating large language models with time-series adaptation techniques for industrial energy forecasting tasks.}
}
@article{YANG2024125211,

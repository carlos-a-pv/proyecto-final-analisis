number = {8},
pages = {258-263},
year = {2024},
note = {6th IFAC Workshop on Advanced Maintenance Engineering, Services and Technology AMEST 2024},
issn = {2405-8963},
doi = {https://doi.org/10.1016/j.ifacol.2024.08.130},
